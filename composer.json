{
  "type": "project",
  "license": "Apache-2.0",
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-sockets": "*",
    "symfony/console": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/http-client": "^5.3",
    "symfony/property-access": "^5.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "replace": {
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
